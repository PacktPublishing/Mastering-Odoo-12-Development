<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <template id="group_list" name="Group Students List">
    <t t-call="web.layout">
      <ul>
        <t t-foreach="group_id.student_ids" t-as="student">
          <li t-esc="student.name"/>
        </t>
      </ul>
    </t>
  </template>
</odoo>